{
  "name": "@pickk/react-excel-download",
  "version": "0.0.1",
  "keywords": [
    "react",
    "reactjs",
    "excel",
    "excel-export",
    "excel-download",
    "typescript",
    "xlsx",
    "csv"
  ],
  "description": "Well-typed React Component for Downloading Excel File",
  "main": "dist/index.js",
  "scripts": {
    "format": "prettier --write **/*.{ts,tsx} --ignore-path ./.prettierignore",
    "lint": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "test": "yarn format && yarn lint",
    "build": "rimraf -rf dist && tsc -p tsconfig.json",
    "prepublish": "yarn test && yarn build",
    "publish": "yarn prepublish && npm publish --public",
    "build:storybook": "build-storybook",
    "storybook": "start-storybook -p 6006",
    "deploy": "yarn build:storybook && git add -f storybook-static/ && git commit -m \"Deploy to GitHub Pages\" && git subtree push --prefix storybook-static origin gh-pages"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@storybook/addon-actions": "^6.3.4",
    "@storybook/addon-essentials": "^6.3.4",
    "@storybook/addon-links": "^6.3.4",
    "@storybook/react": "^6.3.4",
    "@types/react": "^17.0.14",
    "@types/xlsx": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "babel-loader": "^8.2.2",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "dependencies": {
    "dayjs": "^1.10.6",
    "xlsx": "^0.17.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DEV-MUGLES/react-excel-download"
  },
  "bugs": {
    "url": "https://github.com/DEV-MUGLES/react-excel-download/issues"
  }
}
